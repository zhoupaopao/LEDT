<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.ledt.MainActivity"
    tools:showIn="@layout/app_bar_main">

    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_height="50dp">
            <Button
                android:id="@+id/abc"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="测试onclick时间（id）" />
            <Button
                android:id="@+id/popup"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="mylist" />

            <Button
                android:id="@+id/showhide"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="showhide" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_height="50dp">
            <Button
                android:onClick="cl"
                android:id="@+id/recycler"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="尝试使用RecyclerView"/>
            <Button
                android:id="@+id/showpic"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="showpic" />
            <Button
                android:id="@+id/showwebView"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="showwebView" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_height="50dp">
            <Button
                android:id="@+id/Carousel"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="测试轮播" />
            <Button
                android:id="@+id/cover"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="覆盖物" />
            <Button
                android:id="@+id/weilan"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="围栏" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_height="50dp">
            <Button
                android:id="@+id/radio_group"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="测试使用radiogroup" />
            <Button
                android:id="@+id/tuisong"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="测试极光推送" />
            <Button
                android:id="@+id/pic_chose_upload"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="选择图片，上传功能" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_height="50dp">
            <Button
                android:id="@+id/dialogui"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="使用dialog_ui" />

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_height="50dp">
            <Button
                android:id="@+id/Navigation_list"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="使用带有右侧导航栏的列表" />
            <Button
                android:id="@+id/Fold_list"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="使用折叠列表" />
            <Button
                android:id="@+id/request_interface"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="请求接口，并显示" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_height="50dp">
            <Button
                android:id="@+id/viewpager_fragment"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="使用viewpager，同时使用简单的fragment" />
            <Button
                android:id="@+id/match_page"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="打开immersionbarApp,配置状态栏和标题栏" />
            <Button
                android:id="@+id/activity_fragment"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="在activity中嵌入一个fragment" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_height="50dp">
            <Button
                android:id="@+id/map_overlay"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="在地图上加覆盖物，但是不影响地图拖动" />

            <Button
                android:id="@+id/file_put"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="文件存储" />
            <Button
                android:id="@+id/SQL_test"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="尝试使用数据库" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_height="50dp">
            <Button
                android:id="@+id/refresh"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="上拉刷新，下拉加载" />

            <Button
                android:id="@+id/location"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="定位,无地图模块" />
            <!--之所以其他的都是大写，是因为textAllCaps-->
            <Button
                android:id="@+id/sever"
                android:textAllCaps="false"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="sever服务" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_height="50dp">
            <Button
                android:id="@+id/list_lan"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="列表图片懒加载,即图片线程池的使用" />

            <Button
                android:id="@+id/shyc"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="列表上滑隐藏，标题栏动画，颜色渐变" />
            <!--&lt;!&ndash;之所以其他的都是大写，是因为textAllCaps&ndash;&gt;-->
            <Button
                android:id="@+id/sylist"
                android:textAllCaps="false"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="有索引的列表" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_height="50dp">
            <Button
                android:id="@+id/left_back"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="侧滑返回" />

            <Button
                android:id="@+id/calendar"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="日历控件" />
            <!--&lt;!&ndash;&lt;!&ndash;之所以其他的都是大写，是因为textAllCaps&ndash;&gt;&ndash;&gt;-->
            <Button
                android:id="@+id/showleft"
                android:textAllCaps="false"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="展示左侧列表" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_height="50dp">
            <Button
                android:id="@+id/hand_calendar"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="手动实现日历，方便熟悉数据库，日历，task，左侧列表使用" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_height="50dp">
            <Button
                android:id="@+id/soap"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="尝试使用soap" />

            <Button
                android:id="@+id/webview"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="webview使用" />
            <!--&lt;!&ndash;&lt;!&ndash;&lt;!&ndash;之所以其他的都是大写，是因为textAllCaps&ndash;&gt;&ndash;&gt;&ndash;&gt;-->
            <Button
                android:id="@+id/webservice"
                android:textAllCaps="false"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="使用webservice获取天气信息" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_height="50dp">
            <Button
                android:id="@+id/postall"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="尝试使用统一请求,和超时请求" />
            <Button
                android:id="@+id/waitthread"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="尝试使用线程，等待线程完成在继续执行" />
            <Button
                android:id="@+id/Synchronized"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="尝试同步任务，使用" />
            <!--<Button-->
                <!--android:id="@+id/webview"-->
                <!--android:layout_width="0dp"-->
                <!--android:textSize="@dimen/button_size"-->
                <!--android:layout_height="match_parent"-->
                <!--android:layout_weight="1"-->
                <!--android:onClick="cl"-->
                <!--android:text="webview使用" />-->
            <!--&lt;!&ndash;&lt;!&ndash;&lt;!&ndash;&lt;!&ndash;之所以其他的都是大写，是因为textAllCaps&ndash;&gt;&ndash;&gt;&ndash;&gt;&ndash;&gt;-->
            <!--<Button-->
                <!--android:id="@+id/webservice"-->
                <!--android:textAllCaps="false"-->
                <!--android:layout_width="0dp"-->
                <!--android:textSize="@dimen/button_size"-->
                <!--android:layout_height="match_parent"-->
                <!--android:layout_weight="1"-->
                <!--android:onClick="cl"-->
                <!--android:text="使用webservice获取天气信息" />-->
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_height="50dp">
            <Button
                android:id="@+id/downloadfile"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="尝试下载文件" />
            <Button
                android:id="@+id/socket"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="尝试使用socket，列表的基础使用，需要先开启服务，打开另外一个app" />
            <Button
                android:id="@+id/bottomdialog"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="尝试使用底部浮框" />

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_height="50dp">
            <Button
                android:id="@+id/Manualadd"
                android:layout_width="0dp"
                android:textSize="@dimen/button_size"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:onClick="cl"
                android:text="手动添加布局" />
            <!--<Button-->
                <!--android:id="@+id/socket"-->
                <!--android:layout_width="0dp"-->
                <!--android:textSize="@dimen/button_size"-->
                <!--android:layout_height="match_parent"-->
                <!--android:layout_weight="1"-->
                <!--android:onClick="cl"-->
                <!--android:text="尝试使用socket，列表的基础使用，需要先开启服务，打开另外一个app" />-->
            <!--<Button-->
                <!--android:id="@+id/bottomdialog"-->
                <!--android:layout_width="0dp"-->
                <!--android:textSize="@dimen/button_size"-->
                <!--android:layout_height="match_parent"-->
                <!--android:layout_weight="1"-->
                <!--android:onClick="cl"-->
                <!--android:text="尝试使用底部浮框" />-->

        </LinearLayout>
    </LinearLayout>
    <TextView
        android:id="@+id/hello"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="hello" />

</LinearLayout>
